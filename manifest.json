  {
    "name": "No Popups",
    "version": "1.0",
    "description": "Block any popups from popping up",
    "manifest_version": 2,
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"

    }
  }
